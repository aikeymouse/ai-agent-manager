{
  "name": "ollama_generate",
  "description": "AI generate assistant powered by Ollama",
  "model": "llama3.1:8b",
  "timeout": 120,
  "base_url": "http://host.docker.internal:11434",
  "model_endpoint": "/api/generate",
  "test_endpoint": "/api/show",
  "stream": true,
  "system_prompt": "You are a helpful AI assistant.",
  "options": {
    "temperature": 0.7,
    "top_p": 0.9,
    "top_k": 40,
    "num_predict": 512,
    "num_ctx": 8192,
    "repeat_penalty": 1.1
  }
}
